<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="hera.flixnow.made.submission">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="hera.flixnow.made.submission.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CapstoneProject"
        tools:ignore="AllowBackup">
        <activity android:name="hera.flixnow.made.submission.ui.components.MainActivityBase">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="hera.flixnow.made.submission.ui.components.detail.ActivityDetailBase" />
    </application>

</manifest>